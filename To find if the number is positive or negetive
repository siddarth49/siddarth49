#include <stdio.h>

int
main ()
{
  int a;
  printf ("To find if the given number is positive or negetive\n");
  scanf ("%d", &a);
  if (a > 0)
    {
      printf ("%dis positive", a);
    }

  else (a < 0);
    {
      printf ("%d is negetive", a);
    }
      return 0;
    }
