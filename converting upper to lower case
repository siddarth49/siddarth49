#include <stdio.h>

int
main ()
{
  char uc, lc;
  int ascii;
  printf ("Enter uppr case ");
  scanf ("%c", &uc);
  ascii = uc;
  lc = ascii + 32;
  printf ("%c lc is ", lc);
  return 0;
}
