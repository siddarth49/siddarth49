#include <stdio.h>
#include<math.h>

void
main ()
{
  int a, b, c, d, e;
  float f;
  printf ("To find average of five nubers");
  scanf ("%d %d %d %d %d", &a, &b, &c, &d, &e);
  f = floor(a + b + c + d + e) / 5;
    printf("the average value is %f",f);
}
