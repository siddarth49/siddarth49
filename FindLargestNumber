#include <stdio.h>


void
main ()
{
  int a, b;
  printf ("To find greast number when two numbers are given \n");
  scanf("%d %d",&a,&b);
  if (a > b)
    {
      printf ("a is greater");
    }
  else
    {
      printf ("b is greater");
    }
}
