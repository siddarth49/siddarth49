#include <stdio.h>
int
main ()
{
  float lenght, breadth;
  float area;
  printf ("Enter the lenght of a Reactangle\n");
  scanf ("%f", &lenght);
  printf ("Enter breadth of a Reactangle\n");
  scanf ("%f", &breadth);
  area = lenght * breadth;
  printf ("%0.1f Area of the Reactangle is ", area);
return 0;
    
}
